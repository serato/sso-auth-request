{
	"name": "serato/sso-auth-request",
	"homepage": "https://github.com/serato/sso-auth-request",
	"description": "Library for handling SSO authorisation requests",
	"type": "library",
	"require": {
		"php" : "^7.1",
		"aws/aws-sdk-php": "^3.0",
		"ramsey/uuid": "^3.0.0",
		"serato/sws-php-sdk": "^4.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0",
		"squizlabs/php_codesniffer": "~3.6",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-phpunit": "^0.11.0",
		"guzzlehttp/guzzle": "^6.0.0",
		"enlightn/security-checker": "^1.4"
	},
	"autoload": {
		"psr-4": {
			"Serato\\SsoRequest\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\SsoRequest\\Test\\": "tests/"
		}
	}
}
